TOP=../..

include $(TOP)/configure/CONFIG
#----------------------------------------
#  ADD MACRO DEFINITIONS AFTER THIS LINE

#=============================
# Build the IOC support library

PROD_IOC += BUIL    

# BUIL    .dbd will be created and installed
DBD += BUIL    .dbd

# BUIL    .dbd will be made up from base.dbd and module dbds listed below:
BUIL    _DBD += base.dbd

# BUIL    _registerRecordDeviceDriver.cpp derives from BUIL    .dbd
BUIL    _SRCS += BUIL    _registerRecordDeviceDriver.cpp

# Build the main IOC entry point on workstation OSs.
BUIL    _SRCS_DEFAULT += BUIL    Main.cpp
BUIL    _SRCS_vxWorks += -nil-

#=============================
# Add support for the following uncommented modules:

# # AUTOSAVE
BUIL    _DBD += autosave.dbd
BUIL    _LIBS += autosave

# # IOC Monitor
BUIL    _DBD += iocmon.dbd
BUIL    _LIBS += iocmon

# # System Monitor
BUIL    _DBD += sysmon.dbd
BUIL    _LIBS += sysmon

# # STD
BUIL    _DBD += std.dbd
BUIL    _LIBS += std

# # ASYN
BUIL    _DBD += asyn.dbd 
BUIL    _DBD += drvAsynSerialPort.dbd
BUIL    _DBD += drvAsynIPPort.dbd
BUIL    _LIBS += asyn

-include ../*.snlprog
#=============================

# Finally link to the EPICS Base libraries
BUIL    _LIBS += $(EPICS_BASE_IOC_LIBS)

#=============================

include $(TOP)/configure/RULES
#----------------------------------------
#  ADD RULES AFTER THIS LINE

